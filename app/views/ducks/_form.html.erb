

<% if flash[:errors] %>
  
  <% flash[:errors].each do |error| %>
    <p><%= error %></p>
  <% end %>
  
<% end %>
<br>

form<%= form_for @duck do |f| %>
  <p><%= f.label :name %>
  <%= f.text_field :name, required: true%></p>
  <p><%= f.label :description %>
  <%= f.text_area :description, required: true%></p>
  
  <p><%= f.label :student_name, "student_name" %>
  <%= f.text_field :student_name, list: "students_autocomplete", required: true %></p>
  <p><%= f.submit "Submit", disable_with: 'Submiting...' %></p>
    <datalist id="students_autocomplete">
      <% Student.all.each do |student| %>
        <option value="<%= student.name %>">
      <% end %>
    <datalist>      

<% end %>